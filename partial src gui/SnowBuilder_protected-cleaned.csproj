<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <AssemblyName>SnowBuilder</AssemblyName>
    <GenerateAssemblyInfo>False</GenerateAssemblyInfo>
    <OutputType>WinExe</OutputType>
    <UseWindowsForms>True</UseWindowsForms>
    <TargetFramework>net472</TargetFramework>
    <Prefer32Bit>True</Prefer32Bit>
  </PropertyGroup>
  <PropertyGroup>
    <LangVersion>Preview</LangVersion>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>
  <PropertyGroup>
    <RootNamespace />
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Aura.zero" />
    <EmbeddedResource Include="Aura.zero" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Siticone.Desktop.UI">
      <HintPath>..\Siticone.Desktop.UI.dll</HintPath>
    </Reference>
    <Reference Include="Siticone.UI">
      <HintPath>..\Siticone.UI.dll</HintPath>
    </Reference>
    <Reference Include="System.Core" />
    <Reference Include="System.Net.Http" />
  </ItemGroup>
</Project>